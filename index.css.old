#space{
  width: 100%;
  height: 100%;
  margin-top: 20px;
}
::-webkit-scrollbar {
    width: 10px;
}
::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
    border-radius: 10px;
}
::-webkit-scrollbar-thumb {
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5); 
}
.demo{
  background: lightskyblue;
  color: black;
  outline: none;
  border: 0px;
  box-shadow: 2px 2px 10px #555;
  margin: 0px;
  padding: 4px;
  padding-top: 24px;
  font-family: Helvetica;
}
.div{
  display: inline-flex;
  margin: 6px 8px 6px 8px;
}
.heading{
  position: absolute;
  background: lightskyblue;
}
.close{
     border-radius: 50px;
    background: #39464e;
    color: #ebeef0;
    border: 2px solid #39464e;
    cursor: pointer;
    outline: 0px;
    font-size: 16px;
}
